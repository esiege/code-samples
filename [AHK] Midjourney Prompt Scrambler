s = % A_space
m = ::1
in =  in the style of
StringSplit, element_Instruments, List_Instruments, `,
StringSplit, element_Styles, List_Styles, `,
StringSplit, element_Themes, List_Themes, `,
StringSplit, element_Stuff, List_Stuff, `,
StringSplit, element_Colors, List_Colors, `,
StringSplit, element_Images, List_Images, `,

F12::


	Random, RandomNum, 1, %element_Instruments0%
	Text = % s element_Instruments%RandomNum% m s
	
	Random, RandomNum, 1, %element_Styles0%
	Text =  % Text element_Styles%RandomNum% s
	
	Random, RandomNum, 1, %element_Themes0%
	Text =  % Text element_Themes%RandomNum% s
	
	Random, RandomNum, 1, %element_Colors0%
	Text =  % Text element_Colors%RandomNum% s
	
	Random, RandomNum, 1, %element_Images0%
	Text = % Text element_Images%RandomNum% s/
	
	MsgBox, % Text
return

F1::

	Loop, 30
	{

	Random, RandomNum, 1, %element_Instruments0%
	Text = % s element_Instruments%RandomNum% m s

	Random, RandomNum, 1, %element_Styles0%
	Text = % element_Styles%RandomNum% s

	Random, RandomNum, 1, %element_Styles0%
	Text = % Text element_Styles%RandomNum% s

	Random, RandomNum, 1, %element_Themes0%
	Text = % Text element_Themes%RandomNum% s
	
	Random, RandomNum, 1, %element_Stuff0%
	Text = % Text element_Stuff%RandomNum% s

	Random, RandomNum, 1, %element_Colors0%
	Text = % Text element_Colors%RandomNum% s
	
	Random, RandomNum, 1, %element_Images0%
	Text = % Text element_Images%RandomNum% s
	
	


		send /
		sleep 600
		send {Tab}
		send ^v
		send {space}
		sleep 100
		send % Text
		sleep 100
		send {space} --q 2 --niji 
		sleep 100
		send {Enter}
		sleep 1200
		

		sleep 280
	}

return



F2::

	Loop, 100
	{

	Random, RandomNum, 1, %element_Instruments0%
	Text = % s element_Instruments%RandomNum% m s

	Random, RandomNum, 1, %element_Styles0%
	Text =  % element_Styles%RandomNum% s

	Random, RandomNum, 1, %element_Styles0%
	Text =  % Text element_Styles%RandomNum% s

	Random, RandomNum, 1, %element_Themes0%
	Text = % Text element_Themes%RandomNum% s
	
	Random, RandomNum, 1, %element_Stuff0%
	Text = % Text element_Stuff%RandomNum% s

	Random, RandomNum, 1, %element_Colors0%
	Text = % Text element_Colors%RandomNum% s
	
	
	


		send /
		sleep 600
		send {Tab}
		send ^v
		send {space}
		sleep 100
		send % Text
		sleep 100
		send {space} --q 2 --ar 2:3 --niji --no buildings
		sleep 100
		send {Enter}
		sleep 1200
		

		sleep 60000
	}

return





	
	
